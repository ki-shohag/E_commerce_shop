@model OnlineTechShop.Models.Admin

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <title>Admin Dashboard</title>
    <meta content="" name="descriptison">
    <meta content="" name="keywords">
    <script src="~/Contents/vendor/jquery/jquery.min.js"></script>
    <script src="~/Contents/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Contents/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <link href="~/Contents/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Contents/vendor/icofont/icofont.min.css" rel="stylesheet">
    <link href="~/Contents/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/Contents/vendor/remixicon/remixicon.css" rel="stylesheet">
    <link href="~/Contents/vendor/venobox/venobox.css" rel="stylesheet">
    <link href="~/Contents/vendor/owl.carousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/Contents/vendor/aos/aos.css" rel="stylesheet">
    <link href="~/Contents/CSS_Files/style.css" rel="stylesheet">
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/smoothness/jquery-ui.css">
    <style>

        .featured-item-box {
            min-height: 150px;
            background: #3b555d;
            min-width: 80px;
            color: white;
            border-radius: 5px;
            padding-top: 30px;
        }
    </style>
</head>
<body>
    <header id="header" class="">
        <div class="container d-flex align-items-center">
            <div class="row justify-content-center">
                <div class="col-12 col-lg-1">

                </div>

                <div class="col-12 col-lg-12">
                    <nav class="nav-menu d-none d-lg-block">

                        <ul>
                            <li class="drop-down">
                                <a href="/AdminList">Admins</a>
                                <ul>
                                    <li><a href="/AdminList/Create">Add Admin</a></li>
                                </ul>
                            </li>
                            <li class="drop-down">
                                <a href="/SalesExecutiveList">Sales Executives</a>
                                <ul>
                                    <li><a href="/SalesExecutiveList/Create">Add Sales Executive</a></li>
                                </ul>
                            </li>
                            <li class="drop-down">
                                <a href="/BuyingAgentList">BuyingAgents</a>
                                <ul>
                                    <li><a href="/BuyingAgentList/Create">Add Buying Agent</a></li>
                                </ul>
                            </li>
                            <li class="drop-down">
                                <a href="/ProductList">Products</a>
                                <ul>
                                    <li><a href="/ProductList/Create">Add Products</a></li>
                                    <li><a href="/ProductList/Edit">Update Products</a></li>
                                    <li><a href="/ProductList/Delete">Remove Products</a></li>
                                </ul>
                            </li>
                            <li class="drop-down">
                                <a href="/Sales">Sales</a>
                            </li>
                            <li class="drop-down">
                                <a href="/Profits">Profits</a>
                            </li>
                            <li class="drop-down">
                                <a href="/AdminSettings">Settings</a>
                            </li>
                            <li class="drop-down">
                                <a href="/AdminProfile">Profile</a>
                            </li>
                            <li class="drop-down">
                                <a href="/Logout">Logout</a>
                            </li>
                        </ul>
                    </nav>
                </div>

            </div>
        </div>
    </header>


    <main id="main ">
        <div class="container">
            <br /><br /><br /><br /><br />
            <div class="row mt-2 justify-content-center">
                <div class="col-12 col-lg-8 text-center">

                    <table>
                        <tr>
                            <td colspan="5" align="center"><h3>@Html.DisplayFor(model => model.FullName)'s Information</h3></td>
                        </tr>&nbsp;&nbsp;
                        <tr>
                            <td rowspan="6">
                                <img src="~/Contents/Upload_Files/Admins/@Html.DisplayFor(model => model.ProfilePic)" alt="" height="200" width="200" />
                            </td>
                            <td rowspan="6">&nbsp;</td>
                            <td>User Name</td>
                            <td rowspan="6" width="10%">&nbsp;</td>
                            <td>@Html.DisplayFor(model => model.UserName)</td>
                        </tr>&nbsp;&nbsp;
                        <tr>
                            <td>Email</td>
                            <td>@Html.DisplayFor(model => model.Email)</td>
                        </tr>&nbsp;&nbsp;
                        <tr>
                            <td>Phone</td>
                            <td>@Html.DisplayFor(model => model.Phone)</td>
                        </tr>&nbsp;&nbsp;
                        <tr>
                            <td>Address</td>
                            <td>@Html.DisplayFor(model => model.JoiningDate)</td>
                        </tr>&nbsp;&nbsp;
                        <tr>
                            <td>Joining Date</td>
                            <td>@Html.DisplayFor(model => model.Address)</td>
                        </tr>&nbsp;&nbsp;
                        <tr>
                            <td>Last Updated</td>
                            <td>@Html.DisplayFor(model => model.LastUpdated)</td>
                        </tr>&nbsp;&nbsp;
                    </table>
                </div>
            </div>
        </div>
    </main>

    <script src="~/Contents/vendor/jquery/jquery.min.js"></script>
    <script src="~/Contents/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Contents/vendor/jquery.easing/jquery.easing.min.js"></script>
    <script src="~/Contents/vendor/php-email-form/validate.js"></script>
    <script src="~/Contents/vendor/waypoints/jquery.waypoints.min.js"></script>
    <script src="~/Contents/vendor/counterup/counterup.min.js"></script>
    <script src="~/Contents/vendor/venobox/venobox.min.js"></script>
    <script src="~/Contents/vendor/owl.carousel/owl.carousel.min.js"></script>
    <script src="~/Contents/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/Contents/vendor/aos/aos.js"></script>
    <script src="~/Contents/Scripts_Files/main.js"></script>
    <script src="~/Contents/Scripts_Files/productSearch.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js">

    <main id="main ">
        <div class="container">
            <br /><br /><br /><br /><br />
            <div class="row mt-2 justify-content-center">
                <div class="col-12 col-lg-8 text-center">
                    <div id="category"></div>
                    <br /><br />
                    <div id="brand"></div>
                    <script>
                        $(document).ready(function () {
                            $.getJSON("/AdminDashboard/GetDataCategory", function (data) {
                                var Names = []
                                var Qts = []
                                for (var i = 0; i < data.length; i++) {
                                    Names.push(data[i].name);
                                    Qts.push(data[i].count);
                                }

                                Highcharts.chart('category', {
                                    chart: {
                                        type: 'column'
                                    },
                                    title: {
                                        text: 'Most Popular Categories'
                                    },
                                    subtitle: {
                                        text: ''
                                    },
                                    xAxis: {
                                        categories: Names
                                    },
                                    yAxis: {
                                        title: {
                                            text: 'Quantity'
                                        }
                                    },
                                    plotOptions: {
                                        line: {
                                            dataLabels: {
                                                enabled: true
                                            },
                                            enableMouseTracking: false
                                        }
                                    },
                                    series: [{
                                        name: 'Total',
                                        colorByPoint: true,
                                        data: Qts
                                    }]
                                });
                            });
                        });


                        $(document).ready(function () {
                            $.getJSON("/AdminDashboard/GetDataBrand", function (data) {
                                var Names = []
                                var Qts = []
                                for (var i = 0; i < data.length; i++) {
                                    Names.push(data[i].name);
                                    Qts.push(data[i].count);
                                }

                                Highcharts.chart('brand', {
                                    chart: {
                                        type: 'column'
                                    },
                                    title: {
                                        text: 'Most Popular Brands'
                                    },
                                    subtitle: {
                                        text: ''
                                    },
                                    xAxis: {
                                        categories: Names
                                    },
                                    yAxis: {
                                        title: {
                                            text: 'Quantity'
                                        }
                                    },
                                    plotOptions: {
                                        line: {
                                            dataLabels: {
                                                enabled: true
                                            },
                                            enableMouseTracking: false
                                        }
                                    },
                                    series: [{
                                        name: 'Total',
                                        colorByPoint: true,
                                        data: Qts
                                    }]
                                });
                            });
                        });


                    </script>
